						<div class="swiper-slide h-bg-2">Horizontal Slide 2</div>
					</div>
					<div class="swiper-pagination swiper-pagination-h"></div>
				</div>
			</div>
			<div class="swiper-slide v-bg-2">Vertical Slide 2</div>
			<div class="swiper-slide v-bg-3">Vertical Slide 3</div>
			<div class="swiper-slide">Vertical Slide 4</div>
			<div class="swiper-slide v-bg-5">Vertical Slide 5</div>
			<div class="swiper-slide">Vertical Slide 6</div>
		</div>
		<div class="swiper-pagination swiper-pagination-v"></div>
	</div>
